        "remove-unused-imports": "bun scripts/remove-unused-imports.ts",
        "remove-unused-exports": "bun scripts/remove-unused-exports.ts",
        "analyze-deps": "depcruise src --output-type json > deps.json; bun an.ts",
        "deploy": "yarn run build && cd build && surge . geometric-art.surge.sh",
        "build": "bunx --bun react-router build",
        "copy-wasm": "mkdir -p build/client/node_modules/canvaskit-wasm/bin/ && cp node_modules/canvaskit-wasm/bin/canvaskit.wasm build/client/node_modules/canvaskit-wasm/bin/",
        "__build": "vite build --outDir build",
        "_build": "esbuild src/run.tsx --loader:.jd=text --bundle --define:process.env=\"{}\" --define:process.env.NODE_ENV=\\\"production\\\" --minify --sourcemap --platform=browser --outdir=./build --external:fs --external:events --external:constants --external:path",
        "_start": "esbuild src/run.tsx --loader:.jd=text --define:process.cwd=\"cwd\" --bundle --define:process.env=\"{}\" --define:process.env.NODE_ENV=\\\"development\\\" --sourcemap --platform=browser --servedir=. --outdir=. --serve=:4353 --external:fs --external:events --external:constants --external:path",
        "start": "vite",
        "vest": "node src/vest/run.js",
        "test": "jest",
        "test:scripts": "jest scripts/",
        "test:extract": "jest scripts/extract-definition",
        "test:remove-unused": "jest scripts/remove-unused-imports",
        "test:unused-exports": "jest scripts/remove-unused-exports",
        "lol": "vite build && cp dist/assets/*.js dist/assets/index.js; cp dist/assets/*.css dist/assets/index.css",
        "build-docs": "node docs-src/build.js"
    },
