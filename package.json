{
    "name": "geometricart",
    "private": true,
    "version": "1.0.0",
    "main": "index.js",
    "license": "MIT",
    "scripts": {
        "deploy": "yarn run build && cp index.html docs && cd docs && surge . geometric-art.surge.sh",
        "build": "esbuild src/run.tsx --loader:.jd=text --bundle --define:process.env=\"{}\" --define:process.env.NODE_ENV=\\\"production\\\" --minify --sourcemap --platform=browser --outdir=./docs",
        "start": "esbuild src/run.tsx --loader:.jd=text --define:process.cwd=\"cwd\" --bundle --define:process.env=\"{}\" --define:process.env.NODE_ENV=\\\"development\\\" --sourcemap --platform=browser --servedir=. --outdir=. --serve=:4353 --external:fs --external:events --external:constants",
        "start-showcase": "esbuild src/showcase/openglShowcase.tsx --loader:.jd=text --define:process.cwd=\"cwd\" --bundle --define:process.env=\"{}\" --define:process.env.NODE_ENV=\\\"development\\\" --minify --sourcemap --platform=browser --servedir=../docs/showcase --outdir=../docs/showcase --serve=:4344 --external:fs --external:events --external:constants",
        "build-showcase": "esbuild src/showcase/openglShowcase.tsx --loader:.jd=text --define:process.cwd=\"cwd\" --bundle --define:process.env=\"{}\" --define:process.env.NODE_ENV=\\\"development\\\" --minify --sourcemap --platform=browser --outdir=../docs/showcase --external:fs --external:events --external:constants",
        "music": "node ../language/bootstrap.js prelude-types ../web/src/plugins/music/music-jd.ts ../web/src/plugins/music/music.jd",
        "canvas": "node ../language/bootstrap.js prelude-types ../web/src/plugins/canvas/canvas-jd.ts ../web/src/plugins/canvas/canvas.jd"
    },
    "dependencies": {
        "@emotion/css": "^11.7.1",
        "@emotion/react": "^11.1.5",
        "json-diff-ts": "^1.2.4",
        "kmeans-js": "^0.1.2",
        "localforage": "^1.9.0",
        "png-metadata": "https://github.com/hometlt/png-metadata",
        "prando": "^6.0.1",
        "react": "^17.0.1",
        "react-color": "^2.19.3",
        "react-colorful": "^5.2.2",
        "react-dom": "^17.0.1",
        "roughjs": "^4.5.0",
        "tinytar": "jaredly/tinytar#tinytar-v0.1.0-gitpkg",
        "typescript": "^4.4.4"
    },
    "jest": {
        "verbose": true
    },
    "devDependencies": {
        "@babel/core": "^7.14.8",
        "@babel/preset-env": "^7.14.8",
        "@babel/preset-react": "^7.14.5",
        "@babel/preset-typescript": "^7.14.5",
        "@babel/types": "^7.12.13",
        "@testing-library/dom": "^8.1.0",
        "@testing-library/jest-dom": "^5.14.1",
        "@testing-library/react": "^12.0.0",
        "@testing-library/user-event": "^13.2.1",
        "@types/react": "^17.0.3",
        "@types/react-color": "^3.0.6",
        "@types/react-dom": "^17.0.2",
        "babel-jest": "^27.0.6",
        "esbuild": "0.10.1",
        "jest": "^27.0.6"
    }
}
