{
    "name": "geometricart",
    "private": true,
    "version": "1.0.0",
    "main": "index.js",
    "license": "MIT",
    "scripts": {
        "deploy": "yarn run build && cp index.html docs && cd docs && surge . geometric-art.surge.sh",
        "build": "esbuild src/run.tsx --loader:.jd=text --bundle --define:process.env=\"{}\" --define:process.env.NODE_ENV=\\\"production\\\" --minify --sourcemap --platform=browser --outdir=./docs",
        "start": "esbuild src/run.tsx --loader:.jd=text --define:process.cwd=\"cwd\" --bundle --define:process.env=\"{}\" --define:process.env.NODE_ENV=\\\"development\\\" --sourcemap --platform=browser --servedir=. --outdir=. --serve=:4353 --external:fs --external:events --external:constants",
        "vest": "node src/vest/run.js",
        "test": "jest"
    },
    "dependencies": {
        "@babel/parser": "^7.16.12",
        "@emotion/css": "^11.7.1",
        "@emotion/react": "^11.1.5",
        "@vanilla-extract/babel-plugin": "^1.1.4",
        "@vanilla-extract/css": "^1.6.8",
        "@vanilla-extract/esbuild-plugin": "^2.0.2",
        "esbuild-plugin-babel": "^0.2.3",
        "fast-deep-equal": "^3.1.3",
        "fast-glob": "^3.2.11",
        "json-diff-ts": "^1.2.4",
        "kmeans-js": "^0.1.2",
        "localforage": "^1.9.0",
        "png-metadata": "https://github.com/neil-morrison44/png-metadata#1de6be3c50402a188ce06a494c91010f14a297b8",
        "prando": "^6.0.1",
        "prettier": "^2.5.1",
        "react": "^17.0.1",
        "react-color": "^2.19.3",
        "react-colorful": "^5.2.2",
        "react-dom": "^17.0.1",
        "react-json-view": "^1.21.3",
        "roughjs": "^4.5.0",
        "sane": "^5.0.1",
        "tinytar": "jaredly/tinytar#tinytar-v0.1.0-gitpkg",
        "typescript": "^4.4.4"
    },
    "jest": {
        "verbose": true,
        "testMatch": [
            "**/?(*.)+(vest|test).[jt]s?(x)"
        ]
    },
    "devDependencies": {
        "@babel/core": "^7.14.8",
        "@babel/preset-env": "^7.14.8",
        "@babel/preset-react": "^7.14.5",
        "@babel/preset-typescript": "^7.16.7",
        "@babel/types": "^7.12.13",
        "@testing-library/dom": "^8.1.0",
        "@testing-library/jest-dom": "^5.14.1",
        "@testing-library/react": "^12.0.0",
        "@testing-library/user-event": "^13.2.1",
        "@types/react": "^17.0.3",
        "@types/react-color": "^3.0.6",
        "@types/react-dom": "^17.0.2",
        "babel-jest": "^27.0.6",
        "esbuild": "0.10.1",
        "jest": "^27.0.6"
    }
}
