{
    "compilerOptions": {
        "jsx": "react",
        "target": "es2022",
        "noEmit": true,
        "strict": true,
        "moduleResolution": "Node",
        "resolveJsonModule": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true
    }
}
