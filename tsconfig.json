{
    "compilerOptions": {
        "jsx": "react-jsx",
        "target": "ESNext",
        "noEmit": true,
        "strict": true,
        "module": "ESNext",
        "moduleResolution": "Node",
        "resolveJsonModule": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "rootDirs": [".", "./.react-router/types"]
    },

    "include": ["**/*", "**/.server/**/*", "**/.client/**/*", ".react-router/types/**/*"]
}
